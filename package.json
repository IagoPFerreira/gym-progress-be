{
	"scripts": {
		"dev": "ts-node-dev src/index.ts",
		"build": "sucrase ./src -d ./dist --transforms imports,jsx,typescript",
		"start": "pm2-runtime ./dist",
		"db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
		"db:create": "npx sequelize-cli db:create",
		"db:create:test": "NODE_ENV=test DB_NAME=gym_progress_test npx sequelize-cli db:create",
		"db:drop": "npx sequelize-cli db:drop",
		"db:drop:test": "NODE_ENV=test npx sequelize-cli db:drop",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:seed": "npx sequelize-cli db:seed:all",
		"db:seed:undo": "npx sequelize-cli db:seed:undo",
		"db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
		"db:rollback": "npx sequelize-cli db:migrate:undo",
		"db:rollback:all": "npx sequelize-cli db:migrate:undo:all",
		"db:instantiate": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
		"test": "echo \"Error: no test specified\" && exit 1",
		"test:unit": "jest",
		"test:unit:watch": "npx jest test/ --watchAll --coverage --verbose",
		"test:integration": "NODE_ENV=test DB_NAME=gym_progress_test mocha",
		"test:integration:watch": "NODE_ENV=test DB_NAME=gym_progress_test mocha -w",
		"test:coverage": "nyc --exclude 'src/database/**' npm run test:integration"
	},
	"keywords": [],
